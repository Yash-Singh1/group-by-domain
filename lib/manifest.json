{
  "name": "Group by Domain",
  "description": "Allows you to group your tabs by domain name",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "tabGroups", "management"],
  "action": {
    "default_popup": "popup.html"
  }
}
